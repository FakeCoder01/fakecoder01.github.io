{
  "name": "skytracker",
  "version": "1.0.0",
  "description": "A beautiful, real-time flight tracking web application similar to FlightAware and FlightRadar24",
  "main": "index.html",
  "scripts": {
    "start": "npx http-server . -p 8080 -o",
    "dev": "npx http-server . -p 3000 -o --cors",
    "serve": "python -m http.server 8000",
    "demo": "npx http-server . -p 8080 -o demo.html",
    "presentation": "npx http-server . -p 8080 -o presentation.html",
    "build": "echo 'No build process needed - static HTML/CSS/JS application'",
    "test": "echo 'No tests configured yet'",
    "lint": "echo 'No linting configured yet'",
    "deploy": "echo 'Deploy to your preferred static hosting service'"
  },
  "keywords": [
    "flight-tracking",
    "aviation",
    "real-time",
    "aircraft",
    "radar",
    "leaflet",
    "maps",
    "opensky-network",
    "web-app",
    "javascript",
    "flight-radar",
    "plane-spotting"
  ],
  "author": {
    "name": "Ayon S",
    "email": "hardcock@tuta.io",
    "url": "https://github.com/FakeCoder01/skytracker"
  },
  "license": "MIT",
  "homepage": "https://skytracker.fakecoder01.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/FakeCoder01/skytracker.git"
  },
  "bugs": {
    "url": "https://github.com/FakeCoder01/skytracker/issues"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "devDependencies": {
    "http-server": "^14.1.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "config": {
    "port": 8080,
    "host": "localhost"
  },
  "files": [
    "index.html",
    "demo.html",
    "config.js",
    "README.md",
    "*.css",
    "*.js"
  ],
  "private": false,
  "preferGlobal": false,
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "ia32",
    "arm64"
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/skytracker"
